/**
 * CSS for the simulationProject screen
 */
 table td.status{
     width: 142px;
     text-align: center;
     vertical-align: middle !important;
     font-size: 1.5em;
 }
 
 table td.status div.progress{
     float: left;
     margin-bottom: 0px;
     width: 100px;
 }
 
 table td.status span.glyphicon{
     top: -2px;
     float:right;
     margin-left: 4px;
 }

 table td.actions{
     /*width: 143px;*/
     width: 210px;
 }
 
 table td .btn-group.actions-group{
     width: 126px;
 }
 
 #singleSimulationsTable, #paramSweepSimulationsTable{
     margin-bottom: 150px;
 }