html, body{
    height: 100%;
}

#new-simulation-parameters-single-table td,
#new-simulation-parameters-sweep-table td,
#edit-simulation-parameters-single-table td,
#edit-simulation-parameters-sweep-table td{
    vertical-align: middle;
}

#new-simulation-parameters-single-table td div.form-group,
#new-simulation-parameters-sweep-table td div.form-group,
#edit-simulation-parameters-single-table td div.form-group,
#edit-simulation-parameters-sweep-table td div.form-group{
    margin-bottom: 0px;
}


.breadcrumb.list-inline.bigger-size{
    font-size: 18px;
    padding-top: 6px;
    padding-bottom: 0px;
    margin: 7px 10px;
}

h1{
    /*color: #73879C;*/
    color: #1b809e;
}

.nav-tabs{
    margin-bottom: 10px;
}

div.information-panel{
    margin: 4px;
}

div.information-panel-title.expandable:hover{
    cursor: pointer;
}

div.information-panel-title.expandable > h4{
    font-size: 1.2em;
    /*color: #1b809e;*/
    color: #73879C;
    border-bottom: 1px solid;
    padding-bottom: 5px;
}


div.information-panel-title.expandable span.glyphicon-triangle-bottom,
div.information-panel-title.expandable span.glyphicon-triangle-top{
    font-size: 0.9em;
}

div.information-panel-title.expandable > span.title{
    font-size: 1.1em;
    /*font-weight: normal;*/
    font-weight: 600;
    color: #73879C;
    border-bottom: 1px solid;
    padding-bottom: 3px;
    margin: 0px;
}

div.information-panel div.information-panel-content{
    margin: 0px 4px;
}

span.title{
    margin: 5px 0px;
    /*font-weight: 600;
      color: #448ba1;*/
    font-weight: normal;
    color: #1b809e;
    /*color: #73879C;*/
    display:block;
}

div.information-panel-content div.content-value{
    margin: 5px 0px;
}

a.navbar-brand.dnse3-logo{
    padding: 5px 5px 5px 15px;
}

a.navbar-brand.dnse3-logo > img{
    height: 100%;
}

#navbar-progress-bar{
    width: 200px;
}

/* Wrapper for page content to push down footer */
#sticky-wrapper {
    min-height: 100%;
    height: auto !important;
    height: 100%;
    margin: 0px auto 0px;
    /** 
     * set a padding value to make room for the footer including
     * its height and the separation
     * the property padding instead of margin avoid overlappings
     */
    padding-bottom: 75px;
    
}

footer {
    background-color: #f5f5f5;
    padding: 5px 20px;
    text-align: center;
    margin-top: -60px; 
    height: 60px;
    width: 100%;
    border: 1px solid #dddddd;
}

footer .container{
    display: inline-block;
    height: 100%;
}

footer img{
    height: 100%;
    /*height: 60px;*/
}

footer ul{
    display:inline-block;
    padding-left: 0px;
    margin-bottom: 0px;
}

footer ul li{
    display:inline;
    margin: 0px 5px;
}

footer ul li span.glyphicon{
    font-size: 1.3em;
}