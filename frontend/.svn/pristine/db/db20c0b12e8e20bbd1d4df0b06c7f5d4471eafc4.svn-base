/**
 * Manager for the modal dialog that deletes a simulation
 */
var InfoModal = {
    
    modalId: "info-modal",
    closeButtonId: "info-modal-close",
    
    init: function(){
        
    },
    
    showModal: function(){
        $('#' + this.modalId).modal('show');
    },
    
    hideModal: function(){
        $('#' + this.modalId).modal('hide');
    }
    
};

